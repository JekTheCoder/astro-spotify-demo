{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "baseUrl": "/home/jeshua/projects/astro/astro-spotify-demo",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/icons/*": [
        "src/components/icons/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}